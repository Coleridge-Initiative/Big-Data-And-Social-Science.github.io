@Article{ishita-10,
  Title                    = {Investigating multi-label classification for human values},
  Author                   = {Ishita, Emi and Oard, Douglas W. and Fleischmann, Kenneth R. and Cheng, An-Shou and Templeton, Thomas Clay},
  Journal                  = {Proceedings of the American Society for Information Science and Technology},
  Year                     = {2010},
  Number                   = {1},
