@ARTICLE{pitman-97,
  author = {J. Pitman and M. Yor},
  title = {{The two-parameter Poisson-Dirichlet distribution derived from a
	stable subordinator}},
  journal = {Annals of Probability},
  year = {1997},
  volume = {25},
  pages = {855--900},
  number = {2},
  abstract = {The two-parameter Poisson-Dirichlet distribution, denoted $\mathsf{PD}(\alpha,
	\theta)$ is a probability distribution on the set of decreasing positive
	sequences with sum 1. The usual Poisson-Dirichlet distribution with
	a single parameter $\theta$, introduced by Kingman, is $\mathsf{PD}(0,
	\theta)$. Known properties of $\mathsf{PD}(0, \theta)$, including
	the Markov chain description due to Vershik, Shmidt and Ignatov,
	are generalized to the two-parameter case. The size-biased random
	permutation of $\mathsf{PD}(\alpha, \theta)$ is a simple residual
	allocation model proposed by Engen in the context of species diversity
	and rediscovered by Perman and the authors in the study of excursions
	of Brownian motion and Bessel processes. For $0 < \alpha < 1, \mathsf{PD}(\alpha,
